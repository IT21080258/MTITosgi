package com.mtit.postal.packagetypeimplement;

public class PackageTypeImpl implements PackageType {

	public int PackageTypeImpl(String string, boolean b, boolean c) {
		int pkgType = 0;
		if (express = true) {
		  pkgType = 1;
		}
		else if (express = false){
			if (fragile = true){
				pkgType = 1;
			}
			else
				pkgType = 2;
		}
		return pkgType;	}

	@Override 
	public String publishSevice() {
		return "Publish service of publisher";
	}

}
